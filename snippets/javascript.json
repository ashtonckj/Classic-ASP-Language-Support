{
    "Console Log": {
        "prefix": "log",
        "body": "console.log($0);",
        "description": "Log to console"
    },
    "Arrow Function": {
        "prefix": "arrow",
        "body": "const ${1:name} = (${2:params}) => {$0};",
        "description": "Arrow function"
    },
    "Event Listener": {
        "prefix": "listener",
        "body": [
            "document.getElementById('${1:id}').addEventListener('${2:click}', function(e) {",
            "\t$0",
            "});"
        ],
        "description": "Add event listener"
    },
    "Get Element By ID": {
        "prefix": "gebi",
        "body": "document.getElementById('$0')",
        "description": "Get element by ID"
    },
    "Query Selector": {
        "prefix": "qs",
        "body": "document.querySelector('$0')",
        "description": "Query selector"
    },
    "Fetch API": {
        "prefix": "fetch",
        "body": [
            "fetch('${1:url}')",
            "\t.then(response => response.json())",
            "\t.then(data => {",
            "\t\t$0",
            "\t})",
            "\t.catch(error => console.error('Error:', error));"
        ],
        "description": "Fetch API call"
    },
    "SetTimeout": {
        "prefix": "timeout",
        "body": [
            "setTimeout(() => {",
            "\t$0",
            "}, ${1:1000});"
        ],
        "description": "Set timeout"
    }
}